st=[]
ope =['[','{','(']
clo =[']',"}",")"]
def check(s):
    for i in range(len(s)):
        if s[i] in ope:
            st.append(s[i])
        elif s[i] in clo:
            last = clo.index(s[i])
            if (len(st) > 0) and (ope[last] == st[len(st) - 1]) :
                st.pop()
            else:
                return ("at position",i+1)
    if len(st) == 0:
        return "good string"
    else:
        return len(s)+1
s = input()
print(check(s))
